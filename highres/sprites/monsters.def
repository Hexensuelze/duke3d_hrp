   anim { frame0 "frame_34" frame1 "frame_43" fps 22 flags 0 } // Shoot
   frame { name "frame_34" tile0 1715 tile1 1717 smoothduration 0.1 }
   
   anim { frame0 "frame_44" frame1 "frame_52" fps 7 flags 0 } // Fly
